{
  "general": {
    "symbols_to_monitor": [
      "BTC/USDT",
      "ETH/USDT",
      "SOL/USDT",
      "XRP/USDT",
      "DOGE/USDT"
    ],
    "timeframes_to_analyze": [
      "5m",
      "15m",
      "1h",
      "4h",
      "1d"
    ],
    "poll_interval_seconds": 300,
    "max_concurrent_tasks": 5,
    "gemini_cooldown_seconds": 300,
    "logging_focus_symbol": "BTC/USDT",
    "log_level": "DEBUG",
    "min_confluence_for_super_signal": 3,
    "min_risk_reward_ratio": 2.0,
    "account_equity": 10000,
    "assumed_fees_pct": 0.001,
    "assumed_slippage_pct": 0.0005,
    "fetcher_limit": 500,
    "min_rows_for_htf": 300
  },

  "exchange_settings": {
    "exchange_specific": {
      "mexc": {
        "base_url": "https://api.mexc.com",
        "kline_endpoint": "/api/v3/klines",
        "max_limit_per_req": 1000,
        "symbol_template": "{base}{quote}",
        "timeframe_map": {
          "5m": "5m",
          "15m": "15m",
          "1h": "1h",
          "4h": "4h",
          "1d": "1d"
        },
        "rate_limit_delay": 0.5
      },
      "kucoin": {
        "base_url": "https://api.kucoin.com",
        "kline_endpoint": "/api/v1/market/candles",
        "max_limit_per_req": 1500,
        "symbol_template": "{base}-{quote}",
        "timeframe_map": {
          "5m": "5min",
          "15m": "15min",
          "1h": "1hour",
          "4h": "4hour",
          "1d": "1day"
        },
        "rate_limit_delay": 0.5
      },
      "okx": {
        "base_url": "https://www.okx.com",
        "kline_endpoint": "/api/v5/market/candles",
        "max_limit_per_req": 300,
        "symbol_template": "{base}-{quote}",
        "timeframe_map": {
          "5m": "5m",
          "15m": "15m",
          "1h": "1H",
          "4h": "4H",
          "1d": "1D"
        },
        "rate_limit_delay": 0.2
      }
    },
    "symbol_map": {
      "BTC/USDT": { "base": "BTC", "quote": "USDT" },
      "ETH/USDT": { "base": "ETH", "quote": "USDT" },
      "SOL/USDT": { "base": "SOL", "quote": "USDT" },
      "XRP/USDT": { "base": "XRP", "quote": "USDT" },
      "DOGE/USDT": { "base": "DOGE", "quote": "USDT" }
    }
  },

  "signal_cache": {
    "default_ttl_hours": 8,
    "ttl_map_hours": {
      "5m": 0.5,
      "15m": 1.5,
      "1h": 4,
      "4h": 24,
      "1d": 72
    }
  },

  "strategy_priority": [
    "SuperSignal Confluence",
    "ConfluenceSniper",
    "DivergenceSniperPro",
    "WhaleReversal",
    "VolumeCatalystPro",
    "PivotConfluenceSniper",
    "BreakoutHunter",
    "IchimokuHybridPro",
    "ChandelierTrendRider",
    "TrendRiderPro",
    "KeltnerMomentumBreakout",
    "VwapMeanReversion",
    "EmaCrossoverStrategy"
  ],

  "indicators": {
    "adx":              { "enabled": true,  "period": 14 },
    "atr":              { "enabled": true,  "period": 14 },
    "bollinger":        { "enabled": true,  "period": 20, "std_dev": 2.0, "squeeze_stats_period": 240, "squeeze_std_multiplier": 1.5 },
    "cci":              { "enabled": true,  "period": 20 },
    "chandelier_exit":  { "enabled": true,  "dependencies": { "atr": { "period": 22 } }, "atr_multiplier": 3.0 },
    "divergence":       { "enabled": true,  "dependencies": { "rsi": { "period": 14 }, "zigzag": { "deviation": 3.0 } } },
    "donchian_channel": { "enabled": true,  "period": 20 },
    "ema_cross":        { "enabled": true,  "short_period": 9,  "long_period": 21 },
    "fast_ma":          { "enabled": true,  "period": 200, "ma_type": "DEMA" },
    "fibonacci":        { "enabled": true,  "dependencies": { "zigzag": { "deviation": 3.0 } } },
    "ichimoku":         { "enabled": true },
    "keltner_channel":  { "enabled": true,  "ema_period": 20, "dependencies": { "atr": { "period": 10 } }, "atr_multiplier": 2.0 },
    "macd":             { "enabled": true,  "fast_period": 12, "slow_period": 26, "signal_period": 9 },
    "mfi":              { "enabled": true,  "period": 14 },
    "obv":              { "enabled": true },
    "patterns":         { "enabled": true },
    "pivots":           { "enabled": true },
    "rsi":              { "enabled": true,  "period": 14 },
    "stochastic":       { "enabled": true,  "k_period": 14 },
    "structure":        { "enabled": true,  "dependencies": { "zigzag": { "deviation": 3.0 } }, "zone_proximity_pct": 0.1 },
    "supertrend":       { "enabled": true,  "period": 10, "multiplier": 3.0, "dependencies": { "atr": { "period": 14 } } },
    "vwap_bands":       { "enabled": true },
    "whales":           { "enabled": true,  "period": 20, "stdev_multiplier": 2.5, "climactic_multiplier": 4.0 },
    "williams_r":       { "enabled": true,  "period": 14 },
    "zigzag":           { "enabled": true,  "deviation": 3.0 }
  },

  "strategies": {
    "SuperSignal Confluence": { "enabled": true },
    "TrendRiderPro": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "default_params": { "min_adx_strength": 25.0, "tactical_tp_rr_ratio": 2.0 },
      "timeframe_overrides": { "5m": { "min_adx_strength": 22.0 }, "1d": { "min_adx_strength": 28.0 } }
    },
    "VwapMeanReversion": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "max_adx_for_reversion": 25.0,
      "min_rr_ratio": 2.0
    },
    "DivergenceSniperPro": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "min_structure_strength": 3,
      "volatility_regimes": { "low_atr_pct_threshold": 1.5, "low_vol_sl_multiplier": 1.0, "high_vol_sl_multiplier": 1.5 }
    },
    "WhaleReversal": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "min_reversal_score": 7,
      "adaptive_proximity_multiplier": 0.5
    },
    "VolumeCatalystPro": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "min_quality_score": 7,
      "atr_sl_multiplier": 1.0
    },
    "BreakoutHunter": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "min_breakout_score": 6,
      "rr_requirements": { "low_score_threshold": 7, "high_rr": 2.5, "default_rr": 2.0 }
    },
    "IchimokuHybridPro": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "default_params": { "min_total_score": 12, "sl_mode": "adaptive" },
      "timeframe_overrides": { "5m": { "min_total_score": 10 }, "1d": { "min_total_score": 14 } }
    },
    "ChandelierTrendRider": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "session_filter": { "enabled": true, "active_hours_utc": [[8, 21]] }
    },
    "KeltnerMomentumBreakout": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "min_momentum_score": 4
    },
    "PivotConfluenceSniper": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "default_params": { "min_rr_ratio": 2.0, "pivot_levels_to_check": ["S1", "S2", "R1", "R2", "P"] },
      "timeframe_overrides": { "5m": { "atr_sl_multiplier": 1.2 }, "1d": { "min_rr_ratio": 2.5 } }
    },
    "ConfluenceSniper": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "min_confluence_score": 5
    },
    "EmaCrossoverStrategy": {
      "enabled": true,
      "htf_confirmation_enabled": true,
      "htf_map": { "5m": "15m", "15m": "1h", "1h": "4h", "4h": "1d" },
      "master_trend_filter": { "enabled": true, "ma_indicator": "fast_ma", "ma_period": 200 }
    }
  }
}
